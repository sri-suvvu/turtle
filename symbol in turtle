import turtle
turtle.bgcolor('white')
turtle.pensize(12)
def curve():
     for i in range(200):
         turtle.right(1)
         turtle.forward(1)
turtle.speed(20)
turtle.color('red','red')

turtle.begin_fill()
turtle.left(140)
turtle.forward(110)
curve()
turtle.speed(20)
turtle.left(120)
curve()
turtle.forward(110)
turtle.end_fill()
